{
    "name": "glibsky/json-mapper",
    "description": "PHP Json Object Mapper",
    "type": "library",
    "require": {
        "php": "^7.2",
        "doctrine/annotations": "^1.8",
        "psr/container": "^1.0"
    },
    "autoload": {
        "files": ["functions.php"],
        "psr-4": {
            "JsonMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit",
        "phpstan": "phpstan analys"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12.0"
    },
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Lukas Grajewsky",
            "email": "grajewski.lukasz@wp.pl"
        }
    ]
}
